.user-setting-userinfo{
    background-color: #F8F8F8;
    padding-bottom: 160px;
}
.user-setting-userinfo .avatar{
    width: 30px;
    height: 30px;
    border-radius: 100px;
}
.user-setting-userinfo fa-panel{
    margin-bottom: 10px;
    display: block;
}
.user_info_section {
    position:relative;
    padding:12px 15px;
    display:flex;
    align-items:center;
    line-height:1.4;
    font-size:14px;
    color: #333;
    justify-content: space-between;
}

.user_info_section picker {
    flex: 1;
    text-align: right;
    color: #666;
}

picker {
    color: #666;
}

.action-sheet-item {
    font-size: 14px;
    line-height: 1.2;
    color: #333;
}

.cancel {
    font-size: 16px;
    line-height: 1.2;
    color: #333;
}